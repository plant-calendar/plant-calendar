{
  "name": "plant-calendar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "fake-local-config": "touch server/local-config.js && echo 'module.exports = {};' > server/local-config.js",
    "overwrite-client-config": "echo \"export const apiBaseUrl = '$BASE_URL';\" > client/config.ts",
    "build-server": "npm run fake-local-config && cd server && tsc && cd .. && mv public backend-dist/public",
    "build-client": "npm run overwrite-client-config && cd client && webpack && cd ..",
    "prestart": "npm run build-server",
    "build-client-watch": "cd client && webpack -w && cd ..",
    "start": "cd backend-dist && node server/main.js && cd .. && npm run build-client",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "node server -e html,js,scss --ignore public --ignore client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plant-calendar/plant-calendar.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plant-calendar/plant-calendar/issues"
  },
  "homepage": "https://github.com/plant-calendar/plant-calendar#readme",
